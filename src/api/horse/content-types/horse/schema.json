{
  "kind": "collectionType",
  "collectionName": "horses",
  "info": {
    "singularName": "horse",
    "pluralName": "horses",
    "displayName": "Horse",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 3
    },
    "slug": {
      "type": "string",
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$",
      "required": true
    },
    "profileImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "createAsParent": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "foaling": {
      "type": "date",
      "default": "2023-11-26",
      "required": true
    },
    "place": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "mare",
        "gelding",
        "stallion"
      ],
      "required": true,
      "default": "mare"
    },
    "mother": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::horse.horse"
    },
    "father": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::horse.horse"
    },
    "news": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::new.new",
      "mappedBy": "horses"
    },
    "veterinarian": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::veterinarian.veterinarian",
      "inversedBy": "horses"
    },
    "vaccinations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::vaccination.vaccination",
      "mappedBy": "horses"
    },
    "farrieries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::farriery.farriery",
      "mappedBy": "horses"
    }
  }
}
